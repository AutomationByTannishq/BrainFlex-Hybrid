<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Brainflex Automation Suite">

  <listeners>
    <listener class-name="config.TestListener"/>
  </listeners>

  <parameter name="targetURL" value="https://admin.brainflex360.com/" />

  <parameter name="targetEnvironment" value="admin" />

    <test name="Verify Admin Login" verbose="1">
      <classes>
        <class name="tests.Web_Tests.Admin.Login_Test">
          <methods>
            <include name="Verify_Admin_Login"></include>
          </methods>
        </class>
      </classes>
    </test>

  </suite>